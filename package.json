{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "chrome >= 92",
    "firefox >= 90",
    "safari >= 15.4",
    "edge >= 92"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "type-check": "tsc --noEmit",
    "translate": "tsx scripts/translation/translate.ts",
    "translate:dry-run": "tsx scripts/translation/translate.ts --dry-run",
    "translate:categories": "tsx scripts/translation/translate.ts --type=category",
    "translate:subcategories": "tsx scripts/translation/translate.ts --type=subcategory",
    "translate:drawings": "tsx scripts/translation/translate.ts --type=drawingImage",
    "translate:by-subcategory": "node --max-old-space-size=8192 node_modules/tsx/dist/cli.mjs scripts/translation/translate-by-subcategory.ts"
  },
  "dependencies": {
