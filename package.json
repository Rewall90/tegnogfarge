    "@react-email/render": "^0.0.15",
    "@sanity/client": "^7.12.1",
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.30.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.1",
    "bcrypt": "^5.1.1",
    "comma-separated-tokens": "^2.0.3",
    "commander": "^14.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "mongodb": "^6.3.0",
    "next": "^14.2.30",
    "next-auth": "^4.24.5",
    "next-intl": "^4.5.2",
    "next-sanity": "^9.0.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "openai": "^6.8.1",
    "posthog-js": "^1.280.1",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^2.1.0",
    "react-error-boundary": "^4.0.13",
    "react-is": "^18.2.0",
