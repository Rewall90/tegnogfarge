    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.17",
    "critters": "^0.0.23",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "eslint": "^8.57.0",
